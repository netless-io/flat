{
  "name": "flat",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "homepage": "https://github.com/netless-io/Flat-native",
  "author": {
    "email": "flat@agora.io",
    "name": "Agora"
  },
  "repository": {
    "url": "https://github.com/netless-io/Flat-native"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack --config webpack/webpack.dev.js",
    "copy:renderBuildCode": "node ./scripts/Copy-render-build-code.js",
    "launch:electron": "node --trace-uncaught ./scripts/Launch-electron.js",
    "build:main": "cross-env NODE_ENV=production webpack --config webpack/webpack.prod.js",
    "build": "yarn copy:renderBuildCode && yarn build:main",
    "pack:win": "node ./scripts/Pack.js win",
    "pack:mac": "node ./scripts/Pack.js mac",
    "ship": "yarn build && node ./scripts/Pack.js",
    "ship:win": "yarn build && node ./scripts/Pack.js win",
    "ship:mac": "yarn build && node ./scripts/Pack.js mac",
    "download:agoraAddon": "node ./scripts/Download-agora-addon.js && yarn move:agoraAddon",
    "download:agoraAddon:mac": "node ./scripts/Download-agora-addon.js --arch=x64 --platform=darwin && yarn move:agoraAddon",
    "download:agoraAddon:win": "node ./scripts/Download-agora-addon.js --arch=x86 --platform=win32 && yarn move:agoraAddon",
    "move:agoraAddon": "node scripts/mv-agora-addon.js",
    "postinstall": "yarn run download:agoraAddon",
    "lint": "lint-staged"
  },
  "devDependencies": {
    "@netless/eslint-plugin": "^1.1.2",
    "@types/fs-extra": "^9.0.11",
    "@types/node": "^15.0.3",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "cross-env": "^7.0.3",
    "dotenv-flow": "^3.2.0",
    "dotenv-flow-webpack": "^1.1.0",
    "electron": "11.3.0",
    "electron-builder": "^22.10.5",
    "electron-devtools-installer": "^3.2.0",
    "electron-notarize": "^1.0.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-webpack-plugin": "^2.5.4",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.8",
    "js-yaml": "^4.1.0",
    "lint-staged": "^11.0.0",
    "move-cli": "^1.2.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.1.2",
    "typescript": "^4.2.4",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "agora-electron-sdk": "3.4.2-rc.1-build.609",
    "electron-updater": "^4.3.8",
    "flat-types": "^0.1.0",
    "fs-extra": "^9.0.1",
    "jquery": "^3.5.1",
    "rxjs": "^6.6.6",
    "semver": "^7.3.5",
    "ts-enum-util": "^4.0.2"
  },
  "license": "MIT"
}
