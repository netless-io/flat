    "electron-notarize": "^1.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.1",
    "js-yaml": "^4.1.0",
    "lint-staged": "^12.3.5",
    "move-cli": "^2.0.0",
    "nodemon": "^2.0.15",
