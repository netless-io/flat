{
  "name": "flat",
  "version": "1.1.0",
  "private": true,
  "description": "",
  "homepage": "https://github.com/netless-io/flat",
  "author": {
    "email": "flat@agora.io",
    "name": "Agora"
  },
  "repository": {
    "url": "https://github.com/netless-io/flat"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack --config webpack/webpack.dev.js",
    "build:main": "cross-env NODE_ENV=production webpack --config webpack/webpack.prod.js",
    "build": "yarn _copy:render-build-code && yarn build:main",
    "build:debug": "cross-env FLAT_DEBUG=debug webpack --config webpack/webpack.debug.js",
    "pack:win": "node ./scripts/pack win",
    "pack:mac": "node ./scripts/pack mac",
    "ship": "yarn build && node ./scripts/pack auto",
    "ship:win": "yarn build && yarn pack:win",
    "ship:mac": "yarn build && yarn pack:mac",
    "postinstall": "yarn _download:agora-addon",
    "lint": "lint-staged",
    "_copy:render-build-code": "node ./scripts/build/copy-render-build-code.js",
    "_move:agora-addon": "node ./scripts/agora/mv-addon.js",
    "_download:agora-addon": "node ./scripts/agora/download-addon.js && yarn _move:agora-addon",
    "_download:agora-addon:mac": "node ./scripts/agora/download-addon.js --arch=x64 --platform=darwin && yarn _move:agora-addon",
    "_download:agora-addon:win": "node ./scripts/agora/download-addon.js --arch=x86 --platform=win32 && yarn _move:agora-addon",
    "_launch:electron": "node --trace-uncaught ./scripts/launch/electron.js"
  },
  "devDependencies": {
    "@netless/eslint-plugin": "1.1.2",
    "@types/fs-extra": "^9.0.11",
    "@types/node": "^15.0.3",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "cross-env": "^7.0.3",
    "dotenv-flow": "^3.2.0",
    "dotenv-flow-webpack": "^1.1.0",
    "electron": "11.3.0",
    "electron-builder": "^22.10.5",
    "electron-devtools-installer": "^3.2.0",
    "electron-notarize": "^1.0.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-webpack-plugin": "^3.0.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.3.2",
    "js-yaml": "^4.1.0",
    "lint-staged": "^11.0.0",
    "move-cli": "^1.2.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.1.2",
    "typescript": "^4.2.4",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "agora-electron-sdk": "^3.4.7-rc.1-build.817",
    "electron-updater": "^4.3.8",
    "fs-extra": "^9.0.1",
    "jquery": "^3.5.1",
    "rxjs": "^6.6.6",
    "semver": "^7.3.5",
    "ts-enum-util": "^4.0.2"
  },
  "license": "MIT"
}
