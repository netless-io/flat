{
  "name": "flat",
  "productName": "Flat",
  "version": "1.7.0",
  "private": true,
  "description": "",
  "homepage": "https://github.com/netless-io/flat",
  "author": {
    "email": "flat@agora.io",
    "name": "Agora"
